#include <iostream>
#include<set>
using namespace std;

int main() {
    set<pair<int,int>> relation;
    int n,a,b;
    
    cout<<"Enter the number of pair";
    cin>>n;
    cout<<"enter"<< n << "pair (a,b)";
    for( int i=0; i<n ; i++)
    {
    cin>>a>>b;
    relation.insert({a,b});
    }
    
    cout<<"Relation elements are";
    cout<<"{";
    for( auto p: relation )
    {
        cout<<"("<<p.first<<p.second<<")";
        
    }
    cout<<"}";
    
    return 0;
}
